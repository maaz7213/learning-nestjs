datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
}

model Bookmark {
  id           Int      @id @default(autoincrement())
  title        String
  url          String
  descripation String
  updatedAt    DateTime @updatedAt
  createdAt    DateTime @default(now())
}
